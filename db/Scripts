CREATE TABLE LOTE(lote_id INTEGER NOT NULL PRIMARY KEY, bem_id INTEGER NOT NULL FOREIGN KEY REFERENCES BEM);

CREATE TABLE LOTE
	(
	lote_id INTEGER NOT NULL CONSTRAINT LOTE_PK PRIMARY KEY,
	bem_id INTEGER NOT NULL CONSTRAINT BEM_foreign_key
	REFERENCES BEM
	);
	
	CREATE TABLE BEM(bem_id INTEGER NOT NULL PRIMARY KEY, bem_descricao VARCHAR(500) NOT NULL, bem_detalhes VARCHAR(500), bem_categoria VARCHAR(100))
	
	DELETE TABLE USUARIO_PJ
	
	CREATE TABLE USUARIO_PF(usuario_cpf CHAR(14) NOT NULL PRIMARY KEY, usuario_nome VARCHAR (100)  NOT NULL, usuario_email VARCHAR (100) NOT NULL);
	CREATE TABLE USUARIO_PJ(usuario_cnpj CHAR (14) NOT NULL PRIMARY KEY, usuario_nome VARCHAR (100) NOT NULL , usuario_email VARCHAR (100) NOT NULL);

DROP TABLE LOTE

CREATE TABLE LEILAO(leilao_id INTEGER NOT NULL PRIMARY KEY, lote_id NOT NULL FOREIGN KEY, leilao_dataInicio DATA NOT NULL, leilao_dataFim DATA NOT NULL, leilao_arremate INTEGER, leilao_vendedor CHAR(14) NOT NULL FOREIGN KEY, leilao_comprador CHAR(14) NOT NULL FOREIGN KEY)

CREATE TABLE LEILAO
	(
	leilao_id INTEGER NOT NULL PRIMARY KEY,
	LOTE_foreign_key INTEGER NOT NULL,
	CONSTRAINT lote_id FOREIGN KEY (LOTE_foreign_key) REFERENCES LOTE(LOTE_ID),
	leilao_dataInicio DATE NOT NULL, 
	leilao_dataFim DATE NOT NULL, 
	leilao_arremate INTEGER,
	leilao_vendedor CHAR(14) NOT NULL, 
	leilao_comprador CHAR(14) NOT NULL
	);

	
	
	